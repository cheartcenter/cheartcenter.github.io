ect){var confirmation_content = jQuery(this).contents().find('.GF_AJAX_POSTBACK').html();if(!confirmation_content){confirmation_content = contents;}setTimeout(function(){jQuery('#gform_wrapper_1').replaceWith(confirmation_content);jQuery(document).trigger('gform_confirmation_loaded', [1]);window['gf_submitting_1'] = false;}, 50);}else{jQuery('#gform_1').append(contents);if(window['gformRedirect']) {gformRedirect();}}jQuery(document).trigger('gform_post_render', [1, current_page]);} );} ); }, false );<\/script>\n\t<!--<video width=\"450\" height=\"450\" autoplay loop muted playsinline>-->\n<!--  <source src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/08\/maple_chat_hi.mp4\" type=\"video\/mp4\" \/>-->\n<!--  <source src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/09\/maple_chat_hi.ogv\" type=\"video\/ogg\" \/>-->\n<!--  <source src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/09\/maple_chat_hi.webm\" type=\"video\/webm\" \/>-->\n<!--<\/video>-->\n<div id=\"maple-anim-wrapper\" style=\"display:none;\">\n\t\t<div id=\"maple-anim-inwrapper\">\n\t\t\t<!-- Phase one  -->\n\t\t\t<div class=\"maple-anim-desinput\" id=\"maple-anim-tofade\">\n\t\t\t\tDescribe the details of your visit today.\n\t\t\t\t<br><br><br><br><br>\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-desinput\" id=\"maple-anim-tocome\">\n\t\t\t\tI woke up with my typical UTI symptoms. I seem to get them all the time. Can you help?\n\t\t\t\t<br><br><br>\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-redbtn\" id=\"maple-anim-rb1\">\n\t\t\t\tSee the doctor now\n\t\t\t<\/div>\n\n\t\t\t<!-- Phase two  -->\n\t\t\t<div class=\"maple-anim-inwhitebox\" id=\"maple-anim-p2first\">\n\t\t\t\tI woke up with my typical UTI symptoms. I seem to get them all the time. Can you help?\n\t\t\t\t<div class=\"maple-anim-rtime\">\n\t\t\t\t\t1:00 pm\n\t\t\t\t<\/div>\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/patient-female.jpg\" class=\"maple-anim-cimage\">\n\t\t\t<\/div>\n\t\t\t<div id=\"maple-anim-tocircle\">\n\t\t\t\t<div id=\"maple-anim-incircle\">\n\n\t\t\t\t<\/div>\n\t\t\t<\/div>\n\t\t\t<div id=\"maple-anim-doctext\">\n\t\t\t\tA doctor will be with you in approx 2 mins\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox\" id=\"maple-anim-p2second\">\n\t\t\t\tSorry you&rsquo;re not feeling well. Would you prefer to video chat?\n\t\t\t\t<div class=\"maple-anim-rtime\">\n\t\t\t\t\t1:02 pm\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"maple-anim-ltime\">\n\t\t\t\t\tDr. Hawkins\n\t\t\t\t<\/div>\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/dr-female.jpg\" class=\"maple-anim-cimage\" id=\"maple-anim-cx2\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox\" id=\"maple-anim-p2third\">\n\t\t\t\tYes, video sounds great.\n\t\t\t\t<div class=\"maple-anim-rtime\">\n\t\t\t\t\t1:03 pm\n\t\t\t\t<\/div>\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/patient-female.jpg\" class=\"maple-anim-cimage\">\n\t\t\t<\/div>\n\n\t\t\t<!-- Phase 3 -->\n\t\t\t<div class=\"maple-anim-callbox\">\n\t\t\t\t<div class=\"maple-anim-incallbox\">\n\t\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/dr-female.jpg\" class=\"maple-anim-callimage\"><br>\n\t\t\t\t\t<span style=\"font-size: 24px;font-family: initial;\">Dr. Hawkins<\/span><br>\n\t\t\t\t\t<span style=\"font-size: 16px;\">Family and ER Physician<br><br><\/span>\n\t\t\t\t<\/div>\n\t\t\t\t<div id=\"maple-anim-reject\">\n\t\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/fa.png\" >\n\t\t\t\t<\/div>\n\t\t\t\t<div id=\"maple-anim-accept\">\n\t\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/fa.png\" >\n\t\t\t\t<\/div>\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-cend\">\n\t\t\t\tCall ended. Duration 5m 2s\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-pharmbox\" id=\"maple-anim-p1\">\n\t\t\t\t<span style=\"font-size: 16px\">Diagnosis<\/span><br>\n\t\t\t\t<span style=\"font-size: 20px;font-family: initial;\">Urinary tract infection<\/span>\n\t\t\t\t<div class=\"maple-anim-triangle\"><\/div>\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/diagnosis.png\" id=\"maple-anim-pala\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-pharmbox\" id=\"maple-anim-p2\">\n\t\t\t\t<span style=\"font-size: 20px;font-family: initial;\">Prescription for Macrobid<\/span><br>\n\t\t\t\t<span style=\"font-size: 16px\">100mg, 2x daily for 7 days<\/span>\n\t\t\t\t<div class=\"maple-anim-triangle\" style=\"border-top: 50px solid #89d4c5;\"><\/div>\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/rx.png\" id=\"maple-anim-rx\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-redbtn\" id=\"maple-anim-rb2\">\n\t\t\t\tChoose pharmacy\n\t\t\t<\/div>\n\n\t\t\t<!-- Phase 4 : The end phase  -->\n\t\t\t<div class=\"maple-anim-select-pharm\">\n                Select a pharmacy near you\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox maple-anim-pharms\" id=\"maple-anim-p4first\">\n\t\t\t\tRoyal Pharmacy<br>\n\t\t\t\t6758 Pottery Avenue<br>\n\t\t\t\t<div class=\"maple-anim-select-button\">Select<\/div>\n\t\t\t\t<div class=\"maple-anim-ltime\">\n\t\t\t\t\tSelect a pharmacy near you\n\t\t\t\t<\/div>\n\t\t\t\t<img class=\"maple-anim-maping\" src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/m1.png\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox maple-anim-pharms\" id=\"maple-anim-p4second\">\n\t\t\t\tCity Lane Pharmacy<br>\n\t\t\t\tMcgregor Drive<br>\n\t\t\t\t<div id=\"maple-anim-tofadex\" class=\"maple-anim-select-button\">Select<\/div>\n\t\t\t\t<img class=\"maple-anim-maping\" src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/m2.png\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox maple-anim-pharms\" id=\"maple-anim-p4third\">\n\t\t\t\tCanadian Pharmacy<br>\n\t\t\t\tParkplace Lane<br>\n\t\t\t\t<div class=\"maple-anim-select-button\">Select<\/div>\n\t\t\t\t<img class=\"maple-anim-maping\" src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/m3.png\">\n\t\t\t<\/div>\n\t\t\t<div class=\"maple-anim-inwhitebox\" id=\"maple-anim-p4fourth\">\n\t\t\t\t<img src=\"https:\/\/www.getmaple.ca\/site-extensions\/maple-chat-animation-master\/images\/thumbsup.png\" id=\"maple-anim-thumbsup\"> We&rsquo;ll send a notification as soon as the pharmacy has your prescription.\n\t\t\t<\/div>\n\t\t<\/div>\n\t<\/div>\n<h3>\n\t\tWe&#8217;ve been proudly featured across the country \u2014 here&#8217;s how our doctors can help you on Maple.\n\t<\/h3>\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_monitor.svg\" width=\"47\" height=\"47\" \/>\n\t\t\t\t\t\t\t<h6>24\/7 access to doctors<\/h6>\n\t\t\t\t\t<p>Maple connects you with a licensed physician or nurse practitioner quickly, so you can get the help or advice you need and move on with your day.<\/p>\n\t\t\t\t\t<p>Our doctors and nurse practitioners are well respected, Canadian licensed family or emergency medicine practitioners passionate about delivering outstanding healthcare \u2014 they can treat many common medical issues on Maple.<\/p>\n<p>Some examples include:<\/p>\n<ul>\n<li><a href=\"https:\/\/www.getmaple.ca\/providers\/dermatologists\/\">Dermatology<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/urinary-tract-infection-treatment\/\">Urinary tract infections<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/pink-eye-treatment\/\">Pink eye<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/erectile-dysfunction-treatment\/\">Erectile dysfunction<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/hives-treatment\/\">Hives<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/sore-throat-treatment\/\">Sore throat<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/migraine-treatment\/\">Migraines<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/diarrhea-treatment\/\">Diarrhea<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/fever-treatment\/\">Fever<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/earache-treatment\/\">Earache<\/a><\/li>\n<li>Sprains &amp; minor injuries<\/li>\n<li>Allergies<\/li>\n<li>Cough<\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/cold-symptoms-treatment\/\">Cold<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/flu-treatment\/\">Flu (Influenza)<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/sinus-infection-treatment\/\">Sinus infections<\/a><\/li>\n<li><a href=\"https:\/\/www.getmaple.ca\/conditions\/respiratory-infection-treatment\/\">Respiratory infections<\/a><\/li>\n<li>and more&#8230;<\/li>\n<\/ul>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_pillbox.svg\" width=\"27\" height=\"47\" \/>\n\t\t\t\t\t\t\t<h6>Prescriptions<\/h6>\n\t\t\t\t\t<p>Doctors can give you a\u00a0<a href=\"https:\/\/www.getmaple.ca\/features\/online-prescriptions\/\">prescription<\/a>\u00a0at their discretion, based on your consultation.<\/p>\n<p>Prescriptions on Maple are digital, and we send them to the pharmacy of your choice for fulfillment.<\/p>\n<p>We can even have them delivered to your door in most cases, at no additional cost to you.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_healthsheet.svg\" width=\"33\" height=\"47\" \/>\n\t\t\t\t\t\t\t<h6>Medical notes<\/h6>\n\t\t\t\t\t<p>Doctors can provide\u00a0<a href=\"https:\/\/www.getmaple.ca\/features\/sick-notes\/\">sick notes<\/a>\u00a0for school or work for short absences, so you can focus on resting instead of sitting in a waiting room.<\/p>\n\t\t\t\t\t<p>They can also provide notes for therapist treatments, like a note for a chiropractor or registered massage therapist.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_system.svg\" width=\"40\" height=\"47\" \/>\n\t\t\t\t\t\t\t<h6>Portable medical records<\/h6>\n\t\t\t\t\t<p>With Maple, you can easily store and manage your medical records, and have peace of mind knowing they&#8217;ll be there when you need them.<\/p>\n\t\t\t\t\t<p>As you see doctors on Maple, we&#8217;ll update your profile to include all of your latest diagnoses, prescriptions, and medical conditions.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t<h3>\n\t\tLearn more about WebMD\n\t<\/h3>\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_cannot_red.svg\" width=\"25px\" height=\"25px\" \/>\n\t\t\t\t\t\t\t<h6>Not a live doctor&#8217;s visit<\/h6>\n\t\t\t\t\t<p>WebMD is a great resource when you&#8217;re looking for valuable health information and tools for your health.<\/p>\n<p>But, you cannot use WebMD to speak with a doctor for real-time advice or treatment options.<\/p>\n\t\t\t\t\t<p>WebMD also offers you an online symptom checker to help self-identify possible conditions related to symptoms.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_cannot_red.svg\" width=\"25px\" height=\"25px\" \/>\n\t\t\t\t\t\t\t<h6>No prescriptions<\/h6>\n\t\t\t\t\t<p>You can find medication information on WebMD for various conditions, but you cannot get a prescription through WebMD.<\/p>\n<p>Only licensed, medical physicians in Canada can prescribe you medications.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_cannot_red.svg\" width=\"25px\" height=\"25px\" \/>\n\t\t\t\t\t\t\t<h6>No medical notes<\/h6>\n\t\t\t\t\t<p>If you&#8217;re looking for a medical note, you&#8217;ll need to speak to a Canadian licensed physician who can assess you and provide a note as necessary.<\/p>\n\t\t\t\t\t<p>Similar to prescriptions, WebMD cannot provide you with a sick note since you&#8217;re not speaking to a physician in real-time.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/06\/ic_cannot_red.svg\" width=\"25px\" height=\"25px\" \/>\n\t\t\t\t\t\t\t<h6>Not a health record-keeping solution<\/h6>\n\t\t\t\t\t<p>If you need a convenient way to access and store your own personal medical records, WebMD does not provide this feature.<\/p>\n\t\t\t\t\t<p>Currently, there&#8217;s only a few providers in Canada that let you securely access and store your private health information.<\/p>\n\t\t\t\t<button  data-more=\"See more\" data-less=\"See less\">See more<\/button>\t\t\t\n\t\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2018\/09\/leaf-red.svg\" width=\"27\" height=\"31\" \/>\t\t\t\t\n\t\t\t\t\t\t<h6>See a doctor in 2 minutes or less. <\/h6>\n\t\t<p>If coverage isn\u2019t available through your province, employer, or insurer, visits start at $49.<\/p>\n\t\t<a href=\"https:\/\/www.getmaple.ca\/pricing\/\" target=\"_self\" role=\"button\" rel=\"noopener noreferrer\">\n\t\t\tMore pricing\n\t\t<\/a>\n\t<h3>Patients love using Maple<\/h3>\n<p>Read our social media reviews, or download the app<\/p>\n\t\t\t\t\t<a href=\"https:\/\/itunes.apple.com\/ca\/app\/maple-24-7-online-doctors\/id1294926209\" target=\"_self\" role=\"button\" aria-label=\"Apple App Store\" rel=\"noopener noreferrer\">\n\t\t\t\t\t\t\t<\/a>\n\t\t\t\t\t<a href=\"https:\/\/play.google.com\/store\/apps\/details?id=com.maplenativeuser\" target=\"_self\" role=\"button\" aria-label=\"Google App Store\" rel=\"noopener noreferrer\">\n\t\t\t\t\t\t\t<\/a>\n<h3>\n\t\tHow does Maple compare with other telehealth services?\n\t<\/h3>\n\t\t\t\t<a href=\"https:\/\/www.getmaple.ca\/compare\/maple-vs-ask-the-doctor\/\" target=\"_self\" itemprop=\"url\" rel=\"noopener noreferrer\">\n\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2019\/03\/Maple-vs.-Ask-the-Doctor.png\" alt=\"Maple-vs-Ask-the-Doctor-compare\" itemprop=\"image\" height=\"683\" width=\"1024\" title=\"Maple-vs-Ask-the-Doctor-compare\" onerror=\"this.style.display='none'\"  \/>\n\t\t\t\t<\/a>\n\t\t\t\t<a href=\"https:\/\/www.getmaple.ca\/compare\/maple-vs-telehealth-ontario\" target=\"_self\" itemprop=\"url\" rel=\"noopener noreferrer\">\n\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2019\/03\/Maple-vs.-Telehealth-Ontario-2.png\" alt=\"Maple-vs-Telehealth-Ontario-compare\" itemprop=\"image\" height=\"683\" width=\"1024\" title=\"Maple-vs-Telehealth-Ontario-compare\" onerror=\"this.style.display='none'\"  \/>\n\t\t\t\t<\/a>\n\t\t\t\t<a href=\"https:\/\/www.getmaple.ca\/compare\/maple-vs-eq-virtual\/\" target=\"_self\" itemprop=\"url\" rel=\"noopener noreferrer\">\n\t\t\t\t<img loading=\"lazy\" src=\"https:\/\/www.getmaple.ca\/site-content\/uploads\/2019\/03\/Maple-vs.-EQ-Virtual.png\" alt=\"Maple-vs-EQ-Virtual-compare\" itemprop=\"image\" height=\"683\" width=\"1024\" title=\"Maple-vs-EQ-Virtual-compare\" onerror=\"this.style.display='none'\"  \/>\n